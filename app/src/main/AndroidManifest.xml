<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cornming.memo">

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/note"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/note"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ModifyRecordAcitvity"></activity>
        <activity android:name=".activity.ManagerGroup" />
        <activity
            android:name=".activity.AddRecordActivity"
            android:label="添加笔记" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>